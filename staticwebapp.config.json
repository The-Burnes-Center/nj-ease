{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/images/*.{png,jpg,gif}", "/css/*", "/_next/*", "/*.{js,css,png,jpg,gif,ico,svg}", "/favicon.ico"]
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "platform": {
    "apiRuntime": "node:18"
  }
} 